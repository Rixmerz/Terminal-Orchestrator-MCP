{
  "name": "terminal-orchestrator-mcp",
  "version": "1.0.0",
  "description": "Intelligent terminal orchestrator MCP server with tmux integration",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "terminal-orchestrator-mcp": "dist/index.js"
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "start": "node dist/index.js",
    "mcp": "node dist/index.js",
    "test": "vitest",
    "lint": "eslint src/**/*.ts",
    "clean": "rm -rf dist",
    "install-scripts": "chmod +x scripts/tmux/*.sh && chmod +x scripts/install.sh"
  },
  "dependencies": {
    "@modelcontextprotocol/sdk": "^1.0.0",
    "chokidar": "^4.0.1",
    "uuid": "^11.0.3",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.9.0",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/eslint-plugin": "^8.15.0",
    "@typescript-eslint/parser": "^8.15.0",
    "eslint": "^9.15.0",
    "typescript": "^5.6.3",
    "vitest": "^2.1.8"
  },
  "keywords": [
    "mcp",
    "tmux",
    "terminal",
    "orchestrator",
    "logging",
    "process-management"
  ],
  "author": "HANA Project",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}